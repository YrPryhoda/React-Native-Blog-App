{
  "expo": {
    "name": "My Blog",
    "slug": "expoio-rn-blog-for-edu",
    "version": "1.0.0",
    "icon": "./assets/blog-icon.png",
    "splash": {
      "image": "./assets/Blog-intro.png",
      "resizeMode": "contain",
      "backgroundColor": "#0ca4b2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.yrpdev.myblogrnappl",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.yrpdev.myblogrnappl",
      "versionCode": 1,
      "permissions":[
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
